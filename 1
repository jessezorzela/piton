maik.ag(float(input('Informe sua agencia:')))
maik.con(float(input('Informe sua conta:')))
maik.sen(float(input('Informe sua senha:')))
maik.deposita(float(input('Informe o valor do deposito:')))
maik.saca(float(input('Informe o valor a ser sacado:')))

print(maik.get_agencia())
print(maik.get_conta())
print(maik.get_senha())
print(maik.get_saldo())